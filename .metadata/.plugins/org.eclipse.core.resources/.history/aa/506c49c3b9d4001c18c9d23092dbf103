package com.kh.model.vo;

import java.sql.Date;

public class Board {
	
	private int bNo;
	private String title;
	private String content;
	private Date create_date;
	private String writer;
	private String delete_yn;
	
	
	public Board() {}
	
	// 게시글 리스트 전체 조회용 생성자
	public Board(int bNo, String title, Date create_date, String writer) {
		this.bNo = bNo;
		this.title = title;
		this.create_date = create_date;
		this.writer = writer;
	}


	public Board(int bNo, String title, String content, Date create_date, String writer, String delete_yn) {
		super();
		this.bNo = bNo;
		this.title = title;
		this.content = content;
		this.create_date = create_date;
		this.writer = writer;
		this.delete_yn = delete_yn;
	}

	public int getbNo() {
		return bNo;
	}

	public void setbNo(int bNo) {
		this.bNo = bNo;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public Date getCreate_date() {
		return create_date;
	}

	public void setCreate_date(Date create_date) {
		this.create_date = create_date;
	}

	public String getWriter() {
		return writer;
	}

	public void setWriter(String writer) {
		this.writer = writer;
	}

	public String getDelete_yn() {
		return delete_yn;
	}

	public void setDelete_yn(String delete_yn) {
		this.delete_yn = delete_yn;
	}

	@Override
	public String toString() {
		return "Board [bNo=" + bNo + ", title=" + title + ", content=" + content + ", create_date=" + create_date
				+ ", writer=" + writer + ", delete_yn=" + delete_yn + "]";
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

}
